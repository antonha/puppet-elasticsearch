# Managed by Puppet, DO NOT TOUCH!

transport:
        tcp:
                port: <%= esTCPPortRange %>

http:
        port: <%= esHTTPPortRange %>

cluster:
        name: "<%= clustername %>"

path:
        logs: <%= esLogPath %>
        data: <%= esDataPath %>

boostrap:
        mlockall: true

<% unless threadpools.nil? -%>
threadpool:
<% threadpools.keys.each do |type| %>
        <%= type %>:
        <% threadpools[type].each do |k,v| -%>
                <%= k %>: <%= v %>
        <% end -%>
<% end -%>
<% end -%>
